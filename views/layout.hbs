          <div class="col-sm-12 col-md-10">
            <div class="box-control">
              <form class="box-search show-icon">
                <span class="icon"><span class="pe-7s-search"></span></span>
                <div class="inner">
                  <input type="text" class="search" placeholder="Search here...">
                  <button class="button-search"><span class="pe-7s-search"></span></button>
                </div>
              </form>
              <div class="mini-cart">
                <a class="cart-link" href="#"><span class="pe-7s-cart"></span> <span
                    class="count">{{totalProduct}}</span></a>
                <div class="show-shopping-cart">
                  <h3 class="title">YOU HAVE <span class="text-primary">({{totalProduct}} ITEMS)</span> IN YOUR
                    CART</h3>
                  <ul class="list-product">
                    {{#if checkUser}}
                    {{#if userCart}}
                    {{#each userCart.selectedProduct as |val index|}}
                    <li>
                      <div class="thumb">
                        <img src="{{val.image}}" alt="">
                      </div>
                      <div class="info">
                        <h4 class="product-name"><a href="/product/{{val._id}}">{{val.name}}</a></h4>
                        <span class="price">{{val.quantity}}x£{{val.price}}</span>
                        <a class="remove-item" href="/carts/deleteproduct/{{index}}"><i class="fa fa-close"></i></a>
                      </div>
                    </li>
                    {{/each}}
                  </ul>
                  <div class="sub-total">
                    Subtotal:£{{userCart.totalPrice}}
                  </div>
                  <div class="group-button">
                    <a href="/carts/shopping_cart" class="button">Shopping Cart</a>
                  </div>
                  {{else}}
                  <div class="box-coupon" style="padding-bottom: 13px;">
                    <div class="inner-box">
                      <h6>Your Shopping Cart Is Empty Please add product <i class="fas fa-sad-tear"></i></h6>
                    </div>
                  </div>
                  {{/if}}
                  {{else}}
                  <div style="text-align: center; margin-top: 2%;">

                    <a href="/users/signin">Please Signin</a>
                  </div>
                  {{/if}}


