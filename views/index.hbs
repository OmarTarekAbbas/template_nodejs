                                </div>
                                <div class="product-info">
                                    <h3 class="product-name"><a href="/product/{{this._id}}">{{this.producttName}}</a>
                                    </h3>
                                    <span class="price">
                                        <ins>£ {{this.price}}</ins>
                                        <del>£ {{this.information.oldPrice}}</del>
                                    </span>
                                </div>
                            </div>
                        </li>
                        {{/each}}
